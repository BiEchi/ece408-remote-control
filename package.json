{
  "name": "ece408-remote-control",
  "displayName": "ECE408 Remote Control",
  "description": "This extension is designed for ECE408 students to push the code to the remote website (webgpu.net) and automatically give a feed back through the built-in browser of vs code.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ece408-remote-control.login",
    "onCommand:ece408-remote-control.pull"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ece408-remote-control.login",
        "title": "login"
      },
      {
        "command": "ece408-remote-control.pull",
        "title": "pull"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.60.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "publisher": "JackBAI",
  "dependencies": {
    "selenium-webdriver": "^4.0.0-rc-1"
  }
}
